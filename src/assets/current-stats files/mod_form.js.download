jQuery(document).ready(function ($) {
    $("body").fadeIn(1000);
    $(".p4-again").mouseover(function () {
        $(".p4-again img").attr('src', 'images/stories/icon/again-h.svg');
    });
    $(".p4-again").mouseleave(function () {
        $(".p4-again img").attr('src', 'images/stories/icon/again.svg');
    });
    $(".p4-backhome").mouseover(function () {
        $(".p4-backhome img").attr('src', 'images/stories/icon/backhome-h.svg');
    });
    $(".p4-backhome").mouseleave(function () {
        $(".p4-backhome img").attr('src', 'images/stories/icon/backhome.svg');
    });
	
});
